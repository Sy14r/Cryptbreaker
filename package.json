{
  "name": "meteor-bootstrap4-react-router4-template",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "test": "npm run test-server && npm run test-client",
    "test-server": "TEST_CLIENT=0 SERVER_TEST_REPORTER=\"list\" meteor test --once --driver-package meteortesting:mocha --port 3100",
    "test-client": "NODE_ENV=UNIT_TESTING jest",
    "prettier": "prettier --write \"client/**/*.js\" \"imports/**/*.js\" \"server/**/*.js\" --single-quote true --trailing-comma es5"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./tests/enzyme-config.js",
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.6.0",
    "@material-ui/core": "^4.8.3",
    "@material-ui/icons": "^4.4.1",
    "aws-sdk": "^2.600.0",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "jquery": "^3.4.1",
    "lodash": "^4.17.11",
    "meteor-node-stubs": "^0.4.1",
    "mui-datatables": "^2.11.0",
    "node-stream-zip": "^1.8.2",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-easy-notifications": "^0.2.1",
    "react-router-dom": "^5.0.1",
    "react-toastify": "^5.5.0",
    "recompose": "^0.30.0",
    "simpl-schema": "^1.5.5",
    "sweetalert2": "^8.17.1",
    "sweetalert2-react-content": "^2.0.2",
    "uniforms": "^2.4.0",
    "uniforms-bridge-json-schema": "^2.4.0",
    "uniforms-material": "^2.4.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.6.0",
    "@meteorjs/eslint-config-meteor": "^1.0.5",
    "autoprefixer": "^9.6.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-preset-react": "^6.24.1",
    "chromedriver": "^74.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-meteor": "^0.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-meteor": "^5.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.3"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "browserlist": [
          "last 2 versions"
        ]
      }
    }
  }
}
